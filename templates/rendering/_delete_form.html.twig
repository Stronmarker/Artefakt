{# templates/rendering/_delete_form.html.twig #}
<form method="post" action="{{ path('app_rendering_delete', {'projectId': projectId, 'id': rendering.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ rendering.id) }}">
    <button class="btn btn-danger">Delete</button>
</form>
